{
  "name": "serve-directory",
  "description": "Serve directory listings",
  "version": "3.0.0",
  "author": "fisker Cheung <lionkay@gmail.com>",
  "license": "MIT",
  "repository": "fisker/serve-directory",
  "dependencies": {
    "accepts": "^1.3.5",
    "debug": "^4.1.1",
    "http-errors": "^1.7.2",
    "lodash.template": "^4.4.0",
    "mime-types": "^2.1.22",
    "opn": "^5.5.0",
    "parseurl": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@xwtec/eslint-config": "^3.7.0",
    "after": "^0.8.2",
    "core-js": "^2.6.5",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "del-cli": "^1.1.0",
    "eslint": "^5.15.2",
    "finalhandler": "^1.1.1",
    "get-port": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.14.0",
    "mocha": "6.0.2",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "serve-static": "^1.13.2",
    "supertest": "^4.0.0"
  },
  "main": "lib/index.common.js",
  "module": "lib/index.esm.js",
  "files": ["lib"],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "clean": "del lib --force",
    "build": "rollup --config",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint src/**/*.js --fix",
    "release": "npm-run-all clean lint build && np --no-yarn --no-cleanup",
    "test": "npm-run-all test:*",
    "test:js": "mocha --reporter spec --bail --check-leaks test/",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
