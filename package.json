{
  "name": "serve-directory",
  "description": "Serve directory listings",
  "version": "2.2.1",
  "author": "fisker Cheung <lionkay@gmail.com>",
  "license": "MIT",
  "repository": "fisker/serve-directory",
  "dependencies": {
    "accepts": "^1.3.5",
    "debug": "^4.1.1",
    "http-errors": "^1.7.2",
    "lodash.template": "^4.4.0",
    "mime-types": "^2.1.22",
    "parseurl": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@xwtec/eslint-config": "^3.7.0",
    "after": "^0.8.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.15.1",
    "finalhandler": "^1.1.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.14.0",
    "mocha": "6.0.2",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "serve-static": "^1.13.2",
    "supertest": "^4.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "update": "yarn upgrade-interactive --latest",
    "build": "cd scripts && sh build.sh",
    "release": "yarn build && np --no-yarn --no-cleanup",
    "test": "npm-run-all test:*",
    "test:js": "mocha --reporter spec --bail --check-leaks test/",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
