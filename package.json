{
  "name": "serve-directory",
  "description": "Serve directory listings",
  "version": "3.1.1",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "repository": "fisker/serve-directory",
  "dependencies": {
    "accepts": "^1.3.7",
    "debug": "^4.1.1",
    "http-errors": "^1.7.3",
    "lodash.template": "^4.5.0",
    "mime-types": "^2.1.24",
    "parseurl": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@commitlint/cli": "^8.0.0",
    "@fisker/commitlint-config": "^1.1.0",
    "@fisker/eslint-config": "^1.2.6",
    "@fisker/eslint-config-mocha": "^1.1.0",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "@fisker/stylelint-config": "^1.1.5",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^6.0.1",
    "esm": "^3.2.25",
    "get-port": "^5.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "markdownlint-cli": "^0.17.0",
    "mocha": "6.1.4",
    "npm-run-all": "^4.1.5",
    "open": "^6.4.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-filesize": "^6.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "stylelint": "^10.1.0",
    "supertest": "^4.0.2"
  },
  "main": "lib/index.common.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "build": "rollup --config",
    "clean": "rimraf lib",
    "dev": "rollup --config --watch",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "example": "cross-env DEBUG=serve-directory node example",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "format:stylelint": "yarn lint:stylelint --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "lint:stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "release": "run-s clean lint build test format dist",
    "test": "run-p test:*",
    "test:js": "mocha --require esm --reporter spec --bail --check-leaks test/"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "sideEffects": false
}
